	<!-- 접속 허용IP -->
	
	<entry key="allow.ip">192.2.0.0/16, 118.46.152.0/24, 101.231.144.0/24</entry>
	
	
<!--  /8 ; A class : /255.0.0.0 과 같음 -->
<!-- /16 ; B class : /255.255.0.0 과 같음 -->
<!-- /24 ; C class : /255.255.255.0 과 같음 -->
<!-- /32 ; 그 ip 만 지정 : /255.255.255.255 와 같음 -->
<!-- -      제로투세븐 본사: 192.2.71.0/24 ~ 192.2.73.0/24 -->
<!-- -      제로투세븐 물류센터: 192.2.70.0/24 -->
<!-- -      메타넷 C/S 센터로 추측(확인 중): 118.46.152, 101.231.144 -->
	
	<!-- 개인정보이력 체크 및 저장 URL -->
	<entry key="read.history.check.url">/mms/member/popup/detail,/mms/member/list,/oms/refund/list,/sps/event/popup/joinList</entry>
